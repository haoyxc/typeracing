{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["warn", 2],
    "no-console": "off",
    "quotes": "off",
    "guard-for-in": "error",
    "no-unused-vars": ["error", { "args": "none", "vars": "local"}],
    "eqeqeq": "error",
    "no-native-reassign": "error"
  }
}
